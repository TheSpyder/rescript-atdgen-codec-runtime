{
  "name": "rescript-atdgen-codec-runtime",
  "version": "0.1.0",
  "description": "ReScript runtime for atdgen",
  "repository": "https://github.com/TheSpyder/rescript-atdgen-codec-runtime",
  "author": "Andrew Herron",
  "license": "MIT",
  "keywords": [
    "atdgen",
    "json",
    "atd",
    "runtime",
    "rescript",
    "ocaml",
    "rescript-atdgen-codec-runtime"
  ],
  "scripts": {
    "prepublishOnly": "yarn test",
    "clean": "rescript clean",
    "build": "yarn generate-tests && rescript",
    "watch": "rescript build -w",
    "test": "yarn build && yarn jest lib/js/__tests__/*_test.bs.js",
    "generate-tests": "cd __tests__ && npx atdgen -t test.atd && npx atdgen -bs test.atd"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.7.0",
    "rescript": "^10.0.0",
    "rescript-atdgen-generator": "^0.1.1"
  },
  "contributors": [
    "Ahrefs <louis.roche@ahrefs.com>"
  ]
}
